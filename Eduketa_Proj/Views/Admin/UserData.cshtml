@model IEnumerable<Eduketa_Proj.Models.User>
@{
    ViewBag.Title = "UserData";
    Layout = "~/Views/Shared/_AdminHome.cshtml";
}

<div class="container">
    <table class="table  table-striped table-dark">
       <tr>
           <th>Id</th>
           <th>Name</th>
           <th>Email</th>
       </tr>
        @foreach (var x in Model)
        {
    <tr>
        <td>@x.id</td>
        <td>@x.name</td>
        <td>@x.email</td>
    </tr>
        }
    </table>
</div>
