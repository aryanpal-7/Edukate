<!--------------------------------------------------------Admin HomePage------------------------------------------------------------------------------->
@{
    ViewBag.Title = "AdminHome";
    Layout = "~/Views/Shared/_AdminHome.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            @if (Session["Adminid"] == null)
            {
                <h1 class="center-block">Welcome Admin Please <a href="/Admin/Login" style="color:Blue">Login</a></h1>

            }
            else
            {
                <h1> Welcome @Session["Adminname"] </h1>
            }


        </div>
    </div>
</div>